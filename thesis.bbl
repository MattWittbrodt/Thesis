    \field{pages}{549S\bibrangedash 554S}
    \field{title}{Dehydration and cognitive performance}
    \field{volume}{26}
    \field{journaltitle}{Journal of the American College of Nutrition}
    \field{month}{10}
    \field{year}{2007}
  \endentry

